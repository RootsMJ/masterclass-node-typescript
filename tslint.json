{
    "extends": "tslint:recommended",
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "no-console": [
            true,
            "error",
            "debug",
            "trace",
            "warn"
        ],
        "no-var-requires": false,
        "no-var-keyword": false,
        "completed-docs": [
            true,
           "classes" 
        ],
        "curly": true,
        "no-empty": false
    }
}